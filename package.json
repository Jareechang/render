{
  "name": "render",
  "version": "1.0.0",
  "description": "Opinionated React template",
  "author": "Omar Elhawary <oedotme@gmail.com>",
  "license": "MIT",
  "keywords": [
    "app",
    "boilerplate",
    "client",
    "context",
    "frontend",
    "graphql",
    "hooks",
    "react",
    "react-router",
    "structure",
    "template",
    "web"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "export": "vite build --outDir dist/static && npm run server && node prerender.mjs",
    "server": "vite build --ssr src/server.tsx --outDir dist/server",
    "preview": "vite preview",
    "compile": "tsc",
    "format": "prettier --write 'src/**/*.{css,html,json,md,mdx,js,jsx,ts,tsx}'",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@tailwindcss/jit": "^0.1.18",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.15",
    "prettier": "^2.3.0",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "vite": "^2.3.1"
  }
}
